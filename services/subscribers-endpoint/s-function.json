{
  "name": "subscribers-endpoint",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: bbluue-audios",
  "customName": false,
  "customRole": false,
  "handler": "subscribers-endpoint/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "subscribers",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${newSubscriberRequest}",
      "responses": "$${responsesForPost}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "URI_SUBSCRIBERS": "${subscribersTableName}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}