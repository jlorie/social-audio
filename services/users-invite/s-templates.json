{
  "responsesForGet": {
    ".*ERROR.*": "$${response400}",
    ".*Process exited.*": "$${response500}",
    ".*Task timed out.*": "$${response504}",
    "default": "$${response200}"
  },
  "responsesForPost": {
    ".*ERROR.*": "$${response400}",
    ".*422.*": "$${response422}",
    ".*Process exited.*": "$${response500}",
    ".*Task timed out.*": "$${response504}",
    "default": "$${response202}"
  },
  "responsesForRedirect": {
    ".*ERROR.*": "$${response400}",
    ".*Process exited.*": "$${response500}",
    ".*Task timed out.*": "$${response504}",
    "default": "$${response302}"
  },
  "response200": {
    "statusCode": "200",
    "responseParameters": "$${responseParameters}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response201": {
    "statusCode": "201",
    "responseParameters": "$${responseParameters}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response202": {
    "statusCode": "202",
    "responseParameters": "$${responseParameters}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response302": {
    "statusCode": "302",
    "responseParameters": "$${responseParametersRedirect}",
    "responseTemplates": {
      "application/json": ""
    }
  },
  "response400": {
    "statusCode": "400",
    "responseModels": {},
    "responseTemplates": {
      "application/json": "#set($inputRoot = $input.path('$.errorMessage'))$inputRoot"
    }
  },
  "response422": {
    "statusCode": "422",
    "responseModels": {},
    "responseTemplates": {
      "application/json": "#set($inputRoot = $input.path('$.errorMessage'))$inputRoot"
    }
  },
  "response500": {
    "statusCode": "500",
    "responseTemplates": {
      "application/json": "{\"error\": \"Server Error\"}"
    }
  },
  "response504": {
    "statusCode": "504",
    "responseTemplates": {
      "application/json": "{\"error\": \"Gateway Timeout\"}"
    }
  },
  "responseParametersRedirect": {
    "method.response.header.location": "integration.response.body.location"
  },
  "responseParameters": {
    "method.response.header.location": "integration.response.body.links.self"
  },
  "requestTemplate": {
    "application/json": "{\"email\": $input.json('$.email')}"
  },
  "inviteRequest": {
    "application/json": "{ \"identity_id\": \"$context.identity.cognitoIdentityId\",  \"emails\": $input.json('$.emails') }"
  }
}