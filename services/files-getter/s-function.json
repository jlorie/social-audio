{
  "name": "files-getter",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: bbluue-audios",
  "customName": false,
  "customRole": false,
  "handler": "files-getter/handler.default",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {},
  "endpoints": [{
    "path": "files/image/{element_id}",
    "method": "GET",
    "type": "AWS",
    "authorizationType": "AWS_IAM",
    "authorizerFunction": false,
    "apiKeyRequired": false,
    "requestParameters": {},
    "requestTemplates": "$${elementRequest}",
    "responses": "$${responsesForRedirect}"
  }],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "MEDIA_URL": "${mediaDitributionUrl}",
    "KEY_PAIR_ID": "${cloudFrontKeyPairId}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
